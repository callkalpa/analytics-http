{
    "id": "HttpAnalytics-Gadget-LineChart",
    "title": "Time Varying Line Chart",
    "type": "gadget",
    "thumbnail": "store://gadget/usa-business-revenue/index.png",
    "data": {
        "url": "store://gadget/HttpAnalytics-Gadget-LineChart/index.xml"
    },
    "listen": {
        "timeRangeChangeSubscriber": {
            "type": "message",
            "description": "This listens to  message received by the subscriber"
        }
    },
    "notify": {
        "chart-zoomed": {
            "type": "date-range",
            "description": "This notifies message generated in publisher"
        }
    },
    "toolbarButtons": {
        "default": {
            "maximize": true,
            "configurations": false
        }
    }
}